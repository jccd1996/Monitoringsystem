<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/temperature_history_graph"
            app:startDestination="@id/dayHistoryTemperatureFragment">

    <fragment android:id="@+id/monthHistoryTemperatureFragment"
              android:name="com.jccd.monitoringsystem.ui.temperature.history.month_history.MonthHistoryTemperatureFragment"
              android:label="fragment_month_history_temperature"
              tools:layout="@layout/fragment_month_history_temperature">
        <argument android:name="type" app:argType="integer" android:defaultValue="0"/>
    </fragment>
    <fragment android:id="@+id/weekHistoryTemperatureFragment"
              android:name="com.jccd.monitoringsystem.ui.temperature.history.week_history.WeekHistoryTemperatureFragment"
              android:label="fragment_week_history_temperature"
              tools:layout="@layout/fragment_week_history_temperature">
    <argument android:name="type" app:argType="integer" android:defaultValue="0"/>
    </fragment>
    <fragment android:id="@+id/dayHistoryTemperatureFragment"
              android:name="com.jccd.monitoringsystem.ui.temperature.history.day_history.DayHistoryTemperatureFragment"
              android:label="fragment_day_history_temperature" tools:layout="@layout/fragment_day_history_temperature">
        <argument android:name="type" app:argType="integer" android:defaultValue="0"/>
    </fragment>
</navigation>